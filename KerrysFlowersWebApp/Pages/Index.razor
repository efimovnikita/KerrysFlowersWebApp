            if (Filter == FilterType.ByColor.ToString())
            {
                FilteredViolets = AppState.Violets
                    .Where(violet => violet.Colors.Contains((VioletColor) Int32.Parse(Criteria)))
                    .ToList();
            }

            InitFilter();

            return;
        }
        */
        
        FilteredViolets = AppState.Violets;
        InitFilter();
    }

    private void InitFilter()
    {
        List<Tag> tagsList = FilteredViolets
            .SelectMany(violet => violet.Tags)
            .Distinct()
            .Select(s => new Tag(s))
            .ToList();

        List<ColorTag> colorsTags = FilteredViolets
            .SelectMany(violet => violet.Colors)
            .Distinct()
            .Select(color => new ColorTag(ExtensionMethods.GetEnumDescription(color), false))
            .ToList();

        List<BreederTag> breedersTags = FilteredViolets
            .Select(violet => violet.Breeder)
            .Distinct()
            .Select(s => new BreederTag(s, false))
            .ToList();

        List<YearTag> yearsTags = FilteredViolets
            .Select(violet => violet.BreedingDate)
            .Distinct()
            .Select(dateTime => new YearTag(dateTime.Year.ToString(), false))
            .ToList();

        List<ChimeraTag> chimeraTags = FilteredViolets
            .Select(violet => violet.IsChimera)
            .Distinct()
            .Select(b => new ChimeraTag(b ? "да" : "нет", false))
            .ToList();

        FilterObject = new Filter(tagsList, colorsTags, breedersTags, yearsTags, chimeraTags);
    }
