@page "/"

<PageTitle>Каталог фиалок Екатерины Ефимовой</PageTitle>

<MainContainer>
    <CatalogHeader />
    <CatalogFilter />
    <CatalogContainer>
        @foreach (Violet violet in AppState.Violets)
        {
            <CatalogCard Violet="violet"/>
        }
    </CatalogContainer>
</MainContainer>


@code
{
    [CascadingParameter] public  CascadingAppState AppState { get; set; }
}