@code
{
    [Parameter] public string Filter { get; set; }
    [Parameter] public string Criteria { get; set; }
    [CascadingParameter] public CascadingAppState AppState { get; set; }

    private List<Violet> _filteredViolets;
    private List<Violet> FilteredViolets
    {
        get
        {
            if (FilterObject == null)
            {
                return _filteredViolets;
            }
            
            return _filteredViolets
                .Where(violet =>
                {
                    List<string> activeFilterTags = FilterObject.Tags
                        .Where(tag => tag.Active)
                        .Select(tag => tag.Name)
                        .ToList();

                    return violet.Tags.All(tag => activeFilterTags.Contains(tag));
                })
                .Where(violet =>
                {
                    List<string> activeBreederTags = FilterObject.BreederTags
                        .Where(tag => tag.Active)
                        .Select(tag => tag.Name)
                        .ToList();

                    return activeBreederTags.Contains(violet.Breeder);
                })
                .Where(violet =>
                {
                    List<string> activeChimeraTags = FilterObject.ChimeraTags
                        .Where(tag => tag.Active)
                        .Select(tag => tag.Name)
                        .ToList();

                    return activeChimeraTags.Contains(violet.IsChimera ? "да" : "нет");
                })
                .Where(violet =>
                {
                    List<string> activeColorTags = FilterObject.ColorTags
                        .Where(tag => tag.Active)
                        .Select(tag => tag.Name)
                        .ToList();

                    return violet.Colors.All(color => activeColorTags.Contains(ExtensionMethods.GetEnumDescription(color)));
                })
                .Where(violet =>
                {
                    List<string> activeColorTags = FilterObject.YearTags
                        .Where(tag => tag.Active)
                        .Select(tag => tag.Name)
                        .ToList();

                    return activeColorTags.Contains(violet.BreedingDate.Year.ToString());
                })
                .ToList();
        }
        set => _filteredViolets = value;
    }

    private Filter FilterObject { get; set; }
    
